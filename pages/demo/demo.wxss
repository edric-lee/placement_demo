.canvas {
	width:90%;
	height:50%;
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
  margin: 20px auto;
  border-radius: 5%;
  
} 
.nav{
  width: 100%;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
}
.nav-title{
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  bottom: 0;
  left: 0;
  position: absolute;
  z-index: 10;
  font-family:PingFang-SC-Medium;
  font-size:18px;
  letter-spacing:2px;
  color:white;
  
}
.box{
  width:100%;
  margin: 0 auto;
}
.canvasBox{
  
}
.reportBox{

  display: flex;
  flex-direction: column;
  align-items: center;
 justify-content:center;
}



.cu-btn{
  font-size: 12px;
}
.divider{
  color:rgba(0, 0, 0, 0.5);
}



















.model{
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.5);/*蒙层的透明度可以自定义*/
  z-index: 9999999999;
}
.show{
  display: block;
}
.close{
  display: none;
}
/*下方可以自定义*/
.model-pack{
  width: 15rem;
  margin: 0 auto;
  margin-top: 50%;
  background: #fff;
  border-radius: 8px;
}
.model-title{
  font-size: 1rem;
  text-align: center;
  padding: 0.3rem 0;
}
.model-content{
  font-size: 0.8rem;
  padding: 0 0.8rem;
}
.model-button{
  font-size: 0.9rem;
  margin-top: 0.3rem;
  width: 100%;
  border-top: 1px solid #eee;
  border-radius: 0 0 8px 8px;
  overflow: hidden;
  text-align: center;
}
.model-button-cancel{
  float: left;
  width: 50%;
  height: 2.5rem;
  line-height: 2.5rem;
  box-sizing: border-box;
  border-right: 1px solid #eee;
}
.model-button-confirm{
  width: 50%;
  float: left;
  height: 2.5rem;
  line-height: 2.5rem;
}
.confirm{
  height: 2.5rem;
  line-height: 2.5rem;
}
.title{
  border: 2rpx black solid;
  width: 100%;
  height: 100rpx;
  text-align: center;
  font-size: 20px;
  color: white;
  border-radius: 150%;
  
}



page {
  background: linear-gradient(#6dd5ed,#2193b0);
}